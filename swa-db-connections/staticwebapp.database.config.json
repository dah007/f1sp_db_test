{
    "$schema": "https://github.com/Azure/data-api-builder/releases/latest/download/dab.draft.schema.json",
    "data-source": {
        "database-type": "mysql",
        "options": {},
        "connection-string": "@env('DATABASE_CONNECTION_STRING_F1SP')"
    },
    "runtime": {
        "rest": {
            "enabled": true,
            "path": "/rest"
        },
        "graphql": {
            "allow-introspection": true,
            "enabled": true,
            "path": "/graphql"
        },
        "host": {
            "mode": "production",
            "cors": {
                "origins": ["http://localhost:4280"],
                "allow-credentials": false
            },
            "authentication": {
                "provider": "StaticWebApps"
            }
        }
    },
    "entities": {
        "circuitWData": {
            "source": {
                "type": "view",
                "key-fields": ["id"],
                "object": "circuits_w_data"
            },
            "permissions": [
                {
                    "actions": ["read"],
                    "role": "anonymous"
                }
            ]
        },
        "circuitWYear": {
